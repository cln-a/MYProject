<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <system:String x:Key="用户名">Name</system:String>
    <system:String x:Key="密码">PassWord</system:String>
    <system:String x:Key="登录">Login IN</system:String>
    <system:String x:Key="工位1">WorkStation One</system:String>
    <system:String x:Key="工位2">WorkStation Two</system:String>
    <system:String x:Key="工位3">WorkStation Three</system:String>
    <system:String x:Key="工位4">WorkStation Four</system:String>
    <system:String x:Key="工位5">WorkStation Five</system:String>
    <system:String x:Key="工位6">WorkStation Six</system:String>
    <system:String x:Key="工位7">WorkStation Seven</system:String>
    <system:String x:Key="工位8">WorkStation Eight</system:String>
    <system:String x:Key="工位9">WorkStation Nine</system:String>
    <system:String x:Key="工位10">WorkStation Ten</system:String>
    <system:String x:Key="工位11">WorkStation Eleven</system:String>
    <system:String x:Key="是否启用：">Enable or Not：</system:String>
    <system:String x:Key="设定产线参数">Set production line parameters</system:String>
    <system:String x:Key="设定耗时时间(单位:s)">Set timeout duration (unit: s)</system:String>
    <system:String x:Key="设定延时时间(单位:s)">Set delay time (unit: s)</system:String>
    <system:String x:Key="设定耗时时间">Set timeout duration</system:String>
    <system:String x:Key="设定延时时间">Set delay time</system:String>
    <system:String x:Key="耗时时间(单位:s)：">Time taken (unit: s):</system:String>
    <system:String x:Key="延时时间(单位:s)：">Delay time (unit: s)：</system:String>
    <system:String x:Key="工位序号">Workstation number</system:String>
    <system:String x:Key="当前耗时">Current time spent</system:String>
    <system:String x:Key="当前工件处理时间（开始）">Current workpiece processing time (start)</system:String>
    <system:String x:Key="当前工件处理时间（结束）">Current workpiece processing time (end)</system:String>
    <system:String x:Key="各工位耗时总览">Overview of time spent at each workstation</system:String>
    <system:String x:Key="取消">Cancel</system:String>
    <system:String x:Key="确认">Confirm</system:String>
    <system:String x:Key="设备未连接">Device not connected</system:String>
    <system:String x:Key="启动工位成功">Successfully started the workstation</system:String>
    

    <!-- 弹出对话框消息 -->
    <system:String x:Key="输入各工位耗时时间不应为0，请重新输入">The timeout duration for each workstation should not be 0, please re-enter</system:String>
    <system:String x:Key="每个工位耗时时间设定成功">Timeout duration for each workstation set successfully</system:String>
    <system:String x:Key="每个工位耗时时间设定失败，请重新设定">Failed to set timeout duration for each workstation, please reset</system:String>
    <system:String x:Key="每个工位耗时时间设定已取消">Timeout duration setting for each workstation has been cancelled</system:String>
    <system:String x:Key="输入各工位延时时间不应为0，请重新输入">The delay time for each workstation should not be 0, please re-enter</system:String>
    <system:String x:Key="每个工位延时时间设定成功">Delay time for each workstation set successfully</system:String>
    <system:String x:Key="每个工位延时时间设定失败，请重新设定">Failed to set delay time for each workstation, please reset</system:String>
    <system:String x:Key="每个工位延时时间设定已取消">Delay time setting for each workstation has been cancelled</system:String>
    <system:String x:Key="系统未响应，请重新启动工位">System not responding, please restart the workstation</system:String>
    <system:String x:Key="是否确认设定每个工位耗时时间为">Are you sure to set the timeout duration for each workstation to</system:String>
    <system:String x:Key="是否确认设定每个工位延时时间为">Are you sure to set the delay time for each workstation to</system:String>
    <system:String x:Key="写入完成">Write completed</system:String>
    <system:String x:Key="Welcome to ApplicationFrameWork">Welcome to ApplicationFrameWork</system:String>
</ResourceDictionary>